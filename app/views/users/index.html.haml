%h1 Listing users

%table
  %thead
    %tr
      %th Username
      %th Uid
      %th Provider
      %th Team
      %th
      %th
      %th
      
  %tbody
    - @users.each do |user|
      %tr
        %td= user.username
        %td= user.uid
        %td= user.provider
        %td= user.team
        %td= link_to 'Show', user
        %td= link_to 'Edit', edit_user_path(user)
        %td= link_to 'Destroy', user, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New User', new_user_path
